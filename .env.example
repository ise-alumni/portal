# Environment Variables Template
# Copy this file to .env.production for production

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_MAPBOX_TOKEN=your_mapbox_token

# Example for local development:
# VITE_SUPABASE_URL=http://127.0.0.1:54321
# VITE_SUPABASE_ANON_KEY=sb_publishable_your_local_key
# VITE_MAPBOX_TOPEN=your_map_box_token

# Example for production:
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your_production_anon_key
# VITE_MAPBOX_TOPEN=your_map_box_token

# Available levels: trace, debug, info, warn, error, silent
VITE_LOG_LEVEL=info

# Enable hydration scan feature (dev only)
VITE_REACT_SCAN_ENABLED=true
