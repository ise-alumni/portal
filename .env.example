# Environment Variables Template
# Copy this file to .env.production for production

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_MAPBOX_TOKEN=your_mapbox_gl_token_here

# Available levels: trace, debug, info, warn, error, silent
VITE_LOG_LEVEL=info

# Enable hydration scan feature (dev only)
VITE_REACT_SCAN_ENABLED=true
